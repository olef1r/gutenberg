{
  "name": "parser",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon --exec babel-node src/index.js",
    "db:create": "node_modules/.bin/sequelize db:create; exit 0;",
    "db:migrate": "node_modules/.bin/sequelize db:migrate",
    "db:rollback": "node_modules/.bin/sequelize db:migrate:undo",
    "db:rollback:all": "node_modules/.bin/sequelize db:migrate:undo:all",
    "db:drop": "node_modules/.bin/sequelize db:drop; exit 0",
    "db:migration": "node_modules/.bin/sequelize migration:generate",
    "db:model": "node_modules/.bin/sequelize model:generate",
    "db:seed": "node_modules/.bin/sequelize seed:generate",
    "test": "mocha test/**/*.js --recursive --require babel-core/register --watch",
    "coverage": "npx nyc npm run test",
    "lint": "eslint src/** test/**; exit 0",
    "prettier": "prettier 'src/**/*.{js,json}' --write && prettier 'test/**/*.{js,json}' --write"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.2.0",
    "eslint": "^7.3.1",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.21.2",
    "mocha": "^8.0.1",
    "nodemon": "^2.0.4",
    "sinon": "^9.0.2"
  },
  "dependencies": {
    "dotenv": "^8.2.0",
    "dotenv-config": "^0.1.1",
    "eslint-plugin-prettier": "^3.1.4",
    "express": "^4.17.1",
    "i": "^0.3.6",
    "lodash": "^4.17.15",
    "npm": "^6.14.5",
    "nyc": "^15.1.0",
    "pg": "^8.2.1",
    "pg-hstore": "^2.3.3",
    "prettier": "^2.0.5",
    "proxyquire": "^2.1.3",
    "sequelize": "^5.22.0",
    "xml2js": "^0.4.23"
  }
}
