{
    "env": {
        "browser": true,
        "es2020": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "rules": {
        "class-methods-use-this": "off",
        "import/prefer-default-export": "off",
        "no-unused-expressions": 0,
        "comma-dangle": "off",
        "wrap-iife": "off",
        "arrow-parens": "off",
        "no-return-assign": "off",
        "function-paren-newline": "off",
        "no-underscore-dangle": "off",
        "object-curly-newline": "off",
        "max-len": [
          "error",
          {
            "code": 120
          }
        ]
    }
}
